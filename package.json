{
  "name": "sbcc-boilerplate",
  "version": "0.0.0",
  "description": "Web application boilerplate",
  "main": "index.js",
  "scripts": {
    "db:create": "env NODE_PATH=. node ./tasks/db/create",
    "db:drop": "env NODE_PATH=. node ./tasks/db/drop",
    "db:migrate": "knex migrate:latest",
    "db:seed": "env NODE_PATH=. node ./tasks/db/seed",
    "start": "env NODE_PATH=. nodemon app/server.js",
    "test": "env NODE_PATH=. jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matortheeternal/sbcc-boilerplate.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matortheeternal/sbcc-boilerplate/issues"
  },
  "homepage": "https://github.com/matortheeternal/sbcc-boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-jetpack": "^5.1.0",
    "knex": "^2.5.1",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "jest": "^29.7.0"
  }
}
